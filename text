<script type="text/javascript">
	var nilOptions = {
    devMode: "true"=="false",
		topReferer : (function() {
			var ref = "";
			var isMobile = function() {
				return window.location.host.indexOf('m.blog.naver.com') >= 0;
			}

			if (isMobile()){
				return document.referrer;
			} else {
				if (ref == '') {
					if (parent != window && parent.location.href == document.referrer) {
						return '';
					} else {
						return document.referrer;
					}
				} else {
					return ref;
				}
			}
		})()
	}
</script>
